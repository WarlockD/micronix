
install : 
	cd Make ; make install
	cd ls ; make install
	cd pwd ; make install
	cd rm ; make install
	cd vi ; make install
	cd mknod ; make install
	cp reset /bin/reset

clean : 
	cd Make ; make clean
	cd ls ; make clean
	cd pwd ; make clean
	cd far ; make clean
	cd rm ; make clean
	cd mknod ; make clean
	cd vi ; make clean
